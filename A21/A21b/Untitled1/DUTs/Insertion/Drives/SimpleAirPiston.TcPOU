<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.11">
  <POU Name="SimpleAirPiston" Id="{0b7d1323-ab3c-4527-8947-07c900a0019d}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK SimpleAirPiston
VAR_INPUT
	bEnable : BOOL;
END_VAR
VAR_OUTPUT
	pistonExtend AT %Q* : BOOL;
	pistonRetract AT %Q* : BOOL;
	
	extended AT %I* : BOOL;
	retracted AT %I* : BOOL;
END_VAR
VAR
	mode : (HOLD, IN, OUT);
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[IF
	NOT bEnable OR
	(retracted AND_THEN (pistonRetract OR mode = OUT)) OR
	(extended AND_THEN (pistonExtend OR mode = IN))
THEN
	mode := HOLD;
END_IF

CASE mode OF
	HOLD:
		pistonRetract := FALSE;
		pistonExtend := FALSE;
		
	IN:
		pistonRetract := FALSE;
		pistonExtend := TRUE;
	
	OUT:
		pistonRetract := TRUE;
		pistonExtend := FALSE;
	
END_CASE]]></ST>
    </Implementation>
    <Method Name="extend" Id="{1445913b-ea25-4077-a571-d047dfa08814}">
      <Declaration><![CDATA[METHOD extend : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[mode := IN;]]></ST>
      </Implementation>
    </Method>
    <Method Name="retract" Id="{4caa366f-5640-4a48-acfd-50fd41a5c7d8}">
      <Declaration><![CDATA[METHOD retract : BOOL
VAR_INPUT
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[mode := OUT;

retract := retracted;]]></ST>
      </Implementation>
    </Method>
    <Method Name="stop" Id="{538ae10f-48c1-4d70-9b3c-2da92f3346c7}">
      <Declaration><![CDATA[METHOD stop : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[pistonExtend := FALSE;
pistonRetract := FALSE;
mode := HOLD;]]></ST>
      </Implementation>
    </Method>
