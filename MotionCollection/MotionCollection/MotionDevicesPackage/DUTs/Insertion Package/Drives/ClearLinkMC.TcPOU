    <Method Name="toHome" Id="{b88c291d-bab6-4c24-87c1-43e3a5206b24}">
      <Declaration><![CDATA[METHOD toHome : BOOL
VAR_INPUT
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF homingDirectionOut THEN
	moveAbsolute(newTarget := maxPosition);
ELSE
	moveAbsolute(newTarget := minPosition);
END_IF]]></ST>
      </Implementation>
    </Method>
